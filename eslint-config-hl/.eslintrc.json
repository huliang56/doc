{
    "env": {
        "browser": true,
        "jquery": true,
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "semi": [1, "always"],
        "quotes": [1, "single"],
        "indent": [1, 2],
        "curly": [2, "multi-line"],
        "no-unmodified-loop-condition": 1,
        "no-self-compare": 1,
        "no-new-wrappers": 1,
        "no-unused-vars": 1,
        "no-loop-func": 1,
        "no-sequences": 1,
        "no-undefined": 1,
        "no-new-func": 1,
        "no-console": 0,
        "no-octal": 1,
        "no-eval": 2,
        "no-void": 1,
        "no-with": 1,
        "eqeqeq": 2,
        "radix": 1,
        "curly": 1
    },
    "globals": {
    }
}
